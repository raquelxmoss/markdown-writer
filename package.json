    "lodash": "^3.10.1",
    "react": "^0.14.6",
    "react-color": "^1.3.0",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "showdown": "^1.3.0"
  },
  "devDependencies": {
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^6.1.0",
