{
  "name": "markdown_editor",
  "version": "1.0.0",
  "description": "I wondered if this was possible...",
  "main": "index.js",
  "scripts": {
    "watch": "watchify index.js -o bundle.js -v -t babelify",
    "build": "browserify index.js -o bundle.js -t babelify",
    "start": "budo index.js:bundle.js --live='*.css' -- -t babelify -g brfs | garnish"
  },
  "keywords": [
    "Editor",
    "Markdown"
  ],
  "author": "Raquel Moss",
  "license": "MIT",
  "dependencies": {
    "babel": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "brfs": "^1.4.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "showdown": "^1.3.0"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "budo": "^6.1.0",
    "garnish": "^4.1.1",
    "watchify": "^3.4.0"
  }
}
